<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mrone.teamone.mapper.ClientCJH">

	<select id="isClient" parameterType="ClientOrderBean">
		SELECT COUNT(*) FROM CL WHERE CL_CODE = #{OS_CLCODE}
	</select>
	
	<select id="isClientPwd" parameterType="ClientOrderBean">
		SELECT COUNT(*) FROM CL WHERE CL_PWD = #{CL_PWD}
	</select>
	
	<insert id="insClientOrder" parameterType="ClientOrderBean">
	
	</insert>
	
</mapper>
